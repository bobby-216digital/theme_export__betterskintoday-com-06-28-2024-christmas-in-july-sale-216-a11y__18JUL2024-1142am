        $(document).on('click', '.close', function () {
            event.stopPropagation();
            $('.sls-selling-plan-group-options').hide();
            $('input#one-time-purchase').prop('checked', '');
            $('input#subscription').prop('checked', '');
        });

        $(document).on('click', '#purchases', function () {
            event.stopPropagation();
            event.preventDefault();

            // alert("purchase");

            jQuery(this).parents(".discount-options").find('li.active').removeClass('active');
            $(this).parent("li").addClass('active');
            jQuery(this).parents(".discount-options").find('.add-to-cart').removeClass("disabled");
            $(this).find('input[type="radio"]').prop("checked", true);
            $('.sls-selling-plan-group-options').hide();
        });

        $(document).on('click', '#subscriptions', function () {
            event.stopPropagation();
            event.preventDefault();

            // alert("subscriptions");
            jQuery(this).parents(".discount-options").find('li.active').removeClass('active');

            jQuery(this).parents(".discount-options").find('.add-to-cart').removeClass("disabled");
            $(this).parent("li").addClass('active');
            $(this).find('input[type="radio"]').prop("checked", true);

            $('.sls-selling-plan-group-options').show();

        });


        $(document).on('click', '.add-to-cart:not(.disabled)', function () {

            var redirectUrl = "";
            var selectedOption = jQuery(this).parents(".discount-options").children("ul").find("li.active").attr("data-val");

            if (selectedOption == "purchase") {
                redirectUrl = jQuery(this).attr("attr-href");
            } else {
                var selectedDiscountOption = jQuery(this).parents(".discount-options").find("select").find(":selected").val();
                redirectUrl = jQuery(this).attr("attr-href") + "&selling_plan=" + selectedDiscountOption;
            }

            window.open(redirectUrl, '_self');
            console.log(redirectUrl);
        });


    });



</script>

